{
  "name": "prettier-plugin-gml",
  "version": "0.2.0",
  "description": "Adds GML language support to Prettier",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/henrylkirk/prettier-plugin-gml.git"
  },
  "keywords": [
    "prettier",
    "prettier-plugin",
    "gml",
    "game-maker",
    "game-maker-studio",
    "game-maker-studio-2",
    "game-maker-language"
  ],
  "author": "Henry Kirk",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/henrylkirk/prettier-plugin-gml/issues"
  },
  "homepage": "https://github.com/henrylkirk/prettier-plugin-gml#readme",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "npx prettier --plugin=./ ./TEST.gml --no-error-on-unmatched-pattern --loglevel=warn",
    "write": "prettier --plugin=./ --write $npm_config_path --ignore-path .prettierignore --no-error-on-unmatched-pattern --loglevel=warn"
  },
  "devDependencies": {
    "prettier": "^2.8.8"
  }
}
